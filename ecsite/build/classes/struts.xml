<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<constant name="struts.devMode" value="true"/>
		<package name="com.internousdev.ecsite.action" extends="struts-default">

		<!-- GoHomeAction -->
		<action name="GoHomeAction" class="com.internousdev.ecsite.action.GoHomeAction"
		method="execute">
			<result name="success">home.jsp</result>
		</action>

		<!-- GoAdminPageAction -->
		<action name="GoAdminPageAction" class="com.internousdev.ecsite.action.GoAdminPageAction"
		method="execute">
			<result name="success">adminPage.jsp</result>
		</action>

		<!-- HomeAction -->
		<action name="HomeAction" class="com.internousdev.ecsite.action.HomeAction"
		method="execute">
			<result name="login">login.jsp</result>
			<result name="success">buyItem.jsp</result>
		</action>

		<!-- LoginAction -->
		<action name="LoginAction" class="com.internousdev.ecsite.action.LoginAction"
		method="execute">
			<result name="success">buyItem.jsp</result>
			<result name="error">login.jsp</result>
			<result name="admin">adminPage.jsp</result>
		</action>

		<!-- UserCreateAction -->
		<action name="UserCreateAction" class="com.internousdev.ecsite.action.UserCreateAction"
		method="execute">
			<result name="success">userCreate.jsp</result>
			<result name="back">login.jsp</result>
		</action>

		<!-- UserCreateConfirmAction -->
		<action name="UserCreateConfirmAction" class="com.internousdev.ecsite.action.UserCreateConfirmAction"
		method="execute">
			<result name="success">userCreateConfirm.jsp</result>
			<result name="error">userCreate.jsp</result>
		</action>

		<!-- UserCreateCompleteAction -->
		<action name="UserCreateCompleteAction" class="com.internousdev.ecsite.action.UserCreateCompleteAction"
		method="execute">
			<result name="success">userCreateComplete.jsp</result>
		</action>

		<!-- BuyItemAction -->
		<action name="BuyItemAction" class="com.internousdev.ecsite.action.BuyItemAction"
		method="execute">
			<result name="success">buyItemConfirm.jsp</result>
			<result name="error">buyItem.jsp</result>
		</action>

		<!-- BuyItemConfirmAction -->
		<action name="BuyItemConfirmAction" class="com.internousdev.ecsite.action.BuyItemConfirmAction"
		method="execute">
			<result name="success">buyItemComplete.jsp</result>
			<result name="back">buyItem.jsp</result>
		</action>

		<!-- MyPageAction -->
		<action name="MyPageAction" class="com.internousdev.ecsite.action.MyPageAction"
		method="execute">
			<result name="success">myPage.jsp</result>
			<result name="error">error.jsp</result>
		</action>

		<!-- LogoutAction -->
		<action name="LogoutAction" class="com.internousdev.ecsite.action.LogoutAction"
		method="execute">
			<result name="success">home.jsp</result>
		</action>

		<!-- GoUserUpdateAction -->
		<action name="GoUserUpdateAction" class="com.internousdev.ecsite.action.GoUserUpdateAction"
		method="execute">
			<result name="success">userUpdateList.jsp</result>
			<result name="error">adminPage.jsp</result>
		</action>

		<!-- UserUpdateAction -->
		<action name="UserUpdateAction" class="com.internousdev.ecsite.action.UserUpdateAction"
		method="execute">
			<result name="success">userUpdate.jsp</result>
		</action>

		<!-- UserUpdateConfirmAction -->
		<action name="UserUpdateConfirmAction" class="com.internousdev.ecsite.action.UserUpdateConfirmAction"
		method="execute">
			<result name="success">userUpdateConfirm.jsp</result>
		</action>

		<!-- ItemCreateAction -->
		<action name="ItemCreateAction" class="com.internousdev.ecsite.action.ItemCreateAction"
		method="execute">
			<result name="success">itemCreate.jsp</result>
		</action>

		<!-- ItemCreateConfirmAction -->
		<action name="ItemCreateConfirmAction" class="com.internousdev.ecsite.action.ItemCreateConfirmAction"
		method="execute">
			<result name="success">itemCreateConfirm.jsp</result>
			<result name="error">itemCreate.jsp</result>
		</action>

		<!-- ItemCreateCompleteAction -->
		<action name="ItemCreateCompleteAction" class="com.internousdev.ecsite.action.ItemCreateCompleteAction"
		method="execute">
			<result name="success">itemCreateComplete.jsp</result>
		</action>

		<!-- ItemDeleteAction -->
		<action name="ItemDeleteAction" class="com.internousdev.ecsite.action.ItemDeleteAction"
		method="execute">
			<result name="success">itemDeleteConfirm.jsp</result>
			<result name="error">itemDeleteList.jsp</result>
		</action>

		<!-- ItemAllDeleteAction -->
		<action name="ItemAllDeleteAction" class="com.internousdev.ecsite.action.ItemAllDeleteAction"
		method="execute">
			<result name="success">itemDeleteList.jsp</result>
		</action>

		<!-- GoItemUpdateAction -->
		<action name="GoItemUpdateAction" class="com.internousdev.ecsite.action.GoItemUpdateAction"
		method="execute">
			<result name="success">itemUpdateList.jsp</result>
		</action>


		<!-- GoItemDeleteAction -->
		<action name="GoItemDeleteAction" class="com.internousdev.ecsite.action.GoItemDeleteAction"
		method="execute">
			<result name="success">itemDeleteList.jsp</result>
		</action>

		</package>

</struts>