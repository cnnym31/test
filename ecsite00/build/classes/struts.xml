<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<constant name="struts.devMode" value="true"/>
	<package name="com.internousdev.ecsite.action" extends="struts-default">


		<action name="AddCartAction" class="com.internousdev.ecsite.action.AddCartAction"
		method="execute">
			<result name="success">cart.jsp</result>
			<result name="error">home.jsp</result>
		</action>

		<action name="BuyItemAction" class="com.internousdev.ecsite.action.BuyItemAction"
		method="execute">
			<result name="success">buyItemConfirm.jsp</result>
			<result name="error">buyItem.jsp</result>
		</action>

		<action name="BuyItemConfirmAction" class="com.internousdev.ecsite.action.BuyItemConfirmAction"
		method="execute">
			<result name="success">buyItemComplete.jsp</result>
			<result name="error">buyItem.jsp</result>
		</action>

		<action name="BuyItemDetailsAction" class="com.internousdev.ecsite.action.BuyItemDetailsAction"
		method="execute">
			<result name="success">buyItemDetails.jsp</result>
		</action>


		<action name="BuyItemListAction" class="com.internousdev.ecsite.action.BuyItemListAction"
		method="execute">
			<result name="success">buyItemList.jsp</result>
		</action>

		<action name="GoAdminPageAction" class="com.internousdev.ecsite.action.GoAdminPageAction"
		method="execute">
			<result name="success">adminPage.jsp</result>
		</action>

		<action name="GoHomeAction" class="com.internousdev.ecsite.action.GoHomeAction"
		method="execute">
			<result name="success">home.jsp</result>
		</action>

		<action name="GoItemListAction" class="com.internousdev.ecsite.action.GoItemListAction"
		method="execute">
			<result name="success">itemList.jsp</result>
		</action>

		<action name="GoLoginPageAction" class="com.internousdev.ecsite.action.GoLoginPageAction"
		method="execute">
			<result name="success">login.jsp</result>
			<result name="login">BuyItemList.jsp</result>
		</action>

		<action name="GoUserListAction" class="com.internousdev.ecsite.action.GoUserListAction"
		method="execute">
			<result name="success">userList.jsp</result>
			<result name="error">adminPage.jsp</result>
		</action>

		<action name="HomeAction" class="com.internousdev.ecsite.action.HomeAction"
		method="execute">
			<result name="login">login.jsp</result>
			<result name="success">buyItemList.jsp</result>
		</action>

		<action name="ItemAllDeleteAction" class="com.internousdev.ecsite.action.ItemAllDeleteAction"
		method="execute">
			<result name="success">itemList.jsp</result>
		</action>

		<action name="ItemCreateAction" class="com.internousdev.ecsite.action.ItemCreateAction"
		method="execute">
			<result name="success">itemCreate.jsp</result>
		</action>

		<action name="ItemCreateConfirmAction" class="com.internousdev.ecsite.action.ItemCreateConfirmAction"
		method="execute">
			<result name="success">itemCreateConfirm.jsp</result>
			<result name="error">itemCreate.jsp</result>
		</action>

		<action name="ItemCreateCompleteAction" class="com.internousdev.ecsite.action.ItemCreateCompleteAction"
		method="execute">
			<result name="success">itemCreateComplete.jsp</result>
		</action>

		<action name="ItemDeleteAction" class="com.internousdev.ecsite.action.ItemDeleteAction"
		method="execute">
			<result name="success">itemDelete.jsp</result>
		</action>

		<action name="ItemDeleteCompleteAction" class="com.internousdev.ecsite.action.ItemDeleteCompleteAction"
		method="execute">
			<result name="success">itemDeleteComplete.jsp</result>
		</action>

		<action name="ItemUpdateAction" class="com.internousdev.ecsite.action.ItemUpdateAction"
		method="execute">
			<result name="success">itemUpdate.jsp</result>
		</action>

		<action name="ItemUpdateConfirmAction" class="com.internousdev.ecsite.action.ItemUpdateConfirmAction"
		method="execute">
			<result name="success">itemUpdateConfirm.jsp</result>
		</action>

		<action name="ItemUpdateCompleteAction" class="com.internousdev.ecsite.action.ItemUpdateCompleteAction"
		method="execute">
			<result name="success">itemUpdateComplete.jsp</result>
		</action>

		<action name="LoginAction" class="com.internousdev.ecsite.action.LoginAction"
		method="execute">
			<result name="success">home.jsp</result>
			<result name="error">login.jsp</result>
			<result name="admin">adminPage.jsp</result>
		</action>

		<action name="LogoutAction" class="com.internousdev.ecsite.action.LogoutAction"
		method="execute">
			<result name="success">home.jsp</result>
		</action>

		<action name="MyPageAction" class="com.internousdev.ecsite.action.MyPageAction"
		method="execute">
			<result name="success">myPage.jsp</result>
		</action>

		<action name="UserCreateAction" class="com.internousdev.ecsite.action.UserCreateAction"
		method="execute">
			<result name="success">userCreate.jsp</result>
			<result name="error">login.jsp</result>
		</action>

		<action name="UserCreateCompleteAction" class="com.internousdev.ecsite.action.UserCreateCompleteAction"
		method="execute">
			<result name="success">userCreateComplete.jsp</result>
		</action>

		<action name="UserCreateConfirmAction" class="com.internousdev.ecsite.action.UserCreateConfirmAction"
		method="execute">
			<result name="success">userCreateConfirm.jsp</result>
			<result name="error">userCreate.jsp</result>
		</action>

		<action name="UserDeleteAction" class="com.internousdev.ecsite.action.UserDeleteAction"
		method="execute">
			<result name="success">userDelete.jsp</result>
		</action>

		<action name="UserDeleteCompleteAction" class="com.internousdev.ecsite.action.UserDeleteCompleteAction"
		method="execute">
			<result name="success">userDeleteComplete.jsp</result>
		</action>

		<action name="UserUpdateAction" class="com.internousdev.ecsite.action.UserUpdateAction"
		method="execute">
			<result name="success">userUpdate.jsp</result>
		</action>

		<action name="UserUpdateConfirmAction" class="com.internousdev.ecsite.action.UserUpdateConfirmAction"
		method="execute">
			<result name="success">userUpdateConfirm.jsp</result>
		</action>

		<action name="UserUpdateCompleteAction" class="com.internousdev.ecsite.action.UserUpdateCompleteAction"
		method="execute">
			<result name="success">userUpdateComplete.jsp</result>
		</action>


	</package>
</struts>